<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data class="CategoryBinding">
        <variable
            name="categorymodel"
            type="com.example.mvvm.viewmodels.CategoryViewModel" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Date:"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="B"
                    android:id="@+id/date"
                    android:layout_below="@+id/mData"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="17sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Daily Confirmed:"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="B"
                    android:id="@+id/dailyconfirmed"
                    android:layout_below="@+id/mData"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="17sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Daily Death:"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="B"
                    android:id="@+id/dailydeceased"
                    android:layout_below="@+id/mData"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="17sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Daily Recovered"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="B"
                    android:id="@+id/dailyrecovered"
                    android:layout_below="@+id/mData"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="17sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Confirmed:"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="B"
                    android:id="@+id/totalconfirmed"
                    android:layout_below="@+id/mData"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="17sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Death:"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="B"
                    android:id="@+id/totaldeceased"
                    android:layout_below="@+id/mData"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="17sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Recovered:"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="30dp"
                    android:textSize="17sp"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="B"
                    android:id="@+id/totalrecovered"
                    android:layout_below="@+id/mData"
                    android:textColor="#000"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textSize="17sp"/>
            </LinearLayout>



        </LinearLayout>
    </LinearLayout>

</layout>